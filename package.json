{
  "name": "@shat/stylenames",
  "version": "1.3.3",
  "description": "A simple JavaScript utility for conditionally joining inline styles together",
  "source": "src/index.js",
  "jsnext:main": "lib/index.module.js",
  "main": "lib/index.js",
  "module": "lib/index.module.js",
  "unpkg": "lib/index.umd.js",
  "scripts": {
    "pretest": "microbundle",
    "test": "jest",
    "watch": "microbundle watch",
    "lint": "xo src tests",
    "format": "xo src tests --fix",
    "build": "npm run clean && microbundle",
    "clean": "rimraf lib",
    "prerelease": "npm run build",
    "release": "np"
  },
  "keywords": [
    "stylenames",
    "inline-styles",
    "styles",
    "css",
    "classname",
    "classnames",
    "util",
    "alpinejs",
    "alpinejs-styles"
  ],
  "devDependencies": {
    "jest": "^29.3.1",
    "microbundle": "^0.14.0",
    "np": "^7.0.0",
    "rimraf": "^3.0.2",
    "xo": "^0.38.2"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "globals": [
      "window",
      "expect",
      "test"
    ]
  },
  "author": "Kevin Mathmann",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shatstack/stylenames.git"
  },
  "bugs": {
    "url": "https://github.com/shatstack/stylenames/issues"
  },
  "homepage": "https://github.com/shatstack/stylenames#readme"
}
